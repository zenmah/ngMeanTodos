{
  "typescript.check.workspaceVersion": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "app/**/.js*": true
   // "app/**/.spec.ts": true
    
  },
  "files.autoSave": "onWindowChange",
"editor.autoClosingBrackets": true,
"tslint.autoFixOnSave": true,
"editor.insertSpaces": true,
"editor.detectIndentation": false
}